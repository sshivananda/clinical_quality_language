			<xs:extension base="QDMBaseType">
				<xs:sequence>
					<xs:element name="authorDatetime" type="dt:DateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="reason" type="dt:Code" minOccurs="0" maxOccurs="1"/>
					<xs:element name="negationRationale" type="dt:Code" minOccurs="0" maxOccurs="1"/>
					<xs:element name="requester" type="QDMEntity" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
